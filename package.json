{
   "name": "vite-solid-ssg-pages",
   "version": "0.0.1",
   "description": "Vite plugin to render some pages of your app statically",
   "main": "./dist/cjs/index.js",
   "type": "module",
   "module": "./dist/es/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      "node": {
         "import": "./dist/es/index.js",
         "require": "./dist/cjs/index.js"
      },
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
   },
   "files": [
      "./dist"
   ],
   "scripts": {
      "build": "rollup -c && tsc"
   },
   "keywords": [
      "solid",
      "ssg"
   ],
   "author": "foxpro tuningiposadka@gmail.com",
   "license": "ISC",
   "homepage": "https://github.com/MrFoxPro/vite-solid-ssg-pages",
   "bugs": {
      "url": "https://github.com/MrFoxPro/vite-solid-ssg-pages/issues"
   },
   "peerDependencies": {
      "vite": "^3",
      "vite-plugin-solid": "^2.3.9"
   },
   "devDependencies": {
      "@babel/preset-typescript": "^7.18.6",
      "@rollup/plugin-babel": "^6.0.0",
      "@rollup/plugin-strip": "^3.0.0",
      "rollup": "^3.2.3",
      "rollup-plugin-delete": "^2.0.0",
      "typescript": "^4.8.4"
   }
}
